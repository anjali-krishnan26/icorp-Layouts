                "Type": "Tax",
                "Key": "Tax Supplier",
                "Name": "Tax-Ignore",
                "Source": "get/Orders/Release/Tax/",
                "SourceRequestBody": [
                  [
                    "Client",
                    "Id"
                  ]
                ],
                "BaseValues": [
                  "Total",
                  "Discount",
                  "AgencyCommission"
                ],
                "Operators": [
                  "add",
                  "subtract",
                  "subtract"
                ],
                "OnChange": {
                  "Calculate": [
                    {
                      "BaseValues": [
                        "Total",
                        "Discount",
                        "AgencyCommission",
                        "this"
