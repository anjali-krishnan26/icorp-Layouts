                "Default": "0",
                "Percentage": false,
                "Name": "GrandTotal",
                "BaseValues": [
                  "Total",
                  "Discount",
                  "AgencyCommission",
                  "Tax",
                  "RoundOff"
                ],
                "Operators": [
                  "add",
                  "subtract",
                  "subtract",
                  "add",
                  "add"
