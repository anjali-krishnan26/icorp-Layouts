                "Get": [
                  "Tax"
                ],
                "hasDefaultOptions": true,
                "Placeholder": "Enter Client Name",
                "OnChange": "TaxInputGenerate('subtotal_tax',{'Client': $('#Id').val()})",
                "Required": 1,
                "Key": "Client Details"
              },
              {
                "Type": "hidden",
                "Name": "Id",
                "Key": "Id"
              },
              {
                "Type": "hidden",
                "Name": "ClientId",
                "Key": "ClientId"
              },
              {
                "Label": "Address",
                "Type": "textarea",
                "Name": "Address",
                "Placeholder": "Enter Office Address",
                "Required": 0,
                "Key": "Address"
              }
            ]
          }
        },
        {
          "Size": 4,
          "Content_Type": "form-elements",
          "Key": "formElementTwo",
          "Content": {
            "Elements": [
              {
                "Label": "GSTIN",
                "Type": "text",
                "Key": "GSTIN",
                "Name": "GSTIN",
