                "Disabled": true
              },
              {
                "Label": "PAN",
                "Type": "text",
                "Key": "PanNo",
                "Name": "PanNo",
                "Placeholder": "Enter PAN",
                "Required": 0,
                "Disabled": true
              },
              {
                "Label": "Due Date",
                "Type": "date",
                "Name": "DueDate",
                "Default": "+15 Days",
                "Placeholder": "",
                "Required": 0,
                "Key": "DueDate"
              }
            ]
          }
        },
        {
          "Size": 4,
          "Content_Type": "form-elements",
          "Key": "formElementThree",
          "Content": {
            "Elements": [
              {
                "Label": "Invoice No. #",
                "Type": "text",
                "Name": "InvoiceNo",
                "Placeholder": "Invoice Number",
                "Required": 1,
                "Key": "InvoiceNo",
                "Disabled": true
              },
              {
                "Label": "Job No.",
                "Type": "select",
                "IsMulti": "true",
                "SelectLimit": 1,
                "Name": "JobId[]",
                "Source": "get/Invoices/JobsToInvoice/",
                "OnClear": {
                  "data": [
                    "Title",
                    "ClientName",
                    "Id",
                    "Address",
                    "GSTIN",
                    "PanNo",
                    "AENo",
                    "JobId[]",
                    "ClientId",
                    "Address"
                  ],
                  "rowData": [
                    {
                      "value": "",
                      "key": [
                        "multipleForm-ignore",
                        0,
                        "Particular[]"
                      ]
                    },
                    {
                      "value": "",
                      "key": [
                        "multipleForm-ignore",
                        0,
                        "Parameters-ignore",
                        0,
                        "Product[]"
                      ]
                    }
                  ]
                },
                "OnChange": {
                  "Name": "SelectedJobs",
                  "Source": "get/Job/detailsForInvoice/",
                  "Response": "Options",
                  "SourceRequestBody": [
                    [
                      "this",
                      "Job"
                    ]
                  ],
                  "ResponseOptions": "JobSelect",
                  "SetState": [
                    "JobSelect"
                  ],
                  "ResponseFill": [
                    {
                      "Title": [
                        "JobData",
                        "Title"
                      ]
                    },
                    {
                      "multipleForm-ignore": [
                        "FillData",
                        "multipleForm-ignore"
                      ]
                    },
                    {
                      "Category": [
                        "FillData",
                        "Category"
                      ]
                    },
                    {
                      "Total": [
                        "FillData",
                        "Total"
                      ]
                    },
                    {
                      "RoundOff": [
                        "FillData",
                        "RoundOff"
                      ]
                    },
                    {
                      "Discount": [
                        "FillData",
                        "Discount"
                      ]
                    },
                    {
                      "DiscountPercentage": [
                        "FillData",
                        "DiscountPercentage"
                      ]
                    },
                    {
                      "GrandTotal": [
                        "FillData",
                        "GrandTotal"
                      ]
                    },
                    {
                      "ClientName": [
                        "ClientData",
                        "Name"
                      ]
                    },
                    {
                      "Id": [
                        "ClientData",
                        "Id"
                      ]
                    },
                    {
                      "Id": [
                        "ClientData",
                        "ClientId"
                      ]
                    },
                    {
                      "Address": [
                        "ClientData",
                        "Address"
                      ]
                    },
                    {
                      "GSTIN": [
                        "ClientData",
                        "GSTIN"
                      ]
                    },
                    {
                      "PanNo": [
                        "ClientData",
                        "PanNo"
                      ]
                    },
                    {
                      "AENo": [
                        "ClientData",
                        "AENo"
                      ]
                    }
                  ]
                },
                "Key": "JobSelector"
              },
              {
                "Label": "Approved Estimate No.",
                "Type": "text",
                "Name": "AENo",
                "Key": "AENo",
                "Placeholder": "Approved Estimate No",
                "Required": 1
              }
            ]
          }
        },
        {
          "Size": 12,
          "Content_Type": "form-elements",
          "Key": "formElementFour",
          "Content": {
            "Elements": [
              {
                "Label": "Title",
                "Name": "Title",
                "Key": "Title",
                "Placeholder": "Enter the job title",
                "Type": "text",
                "Required": 1
              }
            ]
          }
        },
        {
          "Size": 12,
          "Content_Type": "form-elements",
          "Key": "formElementSix",
          "Content": {
            "Elements": [
              {
                "Label": "Header",
                "Name": "Header",
                "Key": "Header",
                "Placeholder": "Enter the job header",
                "Type": "text",
                "Required": 0
              }
            ]
          }
        },
        {
          "Size": 12,
          "Content_Type": "form-elements",
          "Key": "formElementFive",
          "Content": {
            "Elements": [
              {
                "Name": "multipleForm-ignore",
                "Type": "multiple",
                "Display": "table",
                "Add": "Add",
                "AddVariant": "success",
                "DeleteIcon": true,
                "DeleteLabel": "Action",
                "DeleteVariant": "danger",
                "Key": "multipleFormOne",
                "Elements": [
                  {
                    "Label": "Particular",
                    "Type": "editor",
                    "Width": "500",
                    "Class": "textarea Particular",
                    "Name": "Particular[]",
                    "Key": "Particular",
                    "Default": ""
                  },
                  {
                    "Type": "parameters",
                    "Width": "200",
                    "Label": "Product Details",
                    "Key": "parameters",
                    "Name": "Parameters-ignore",
                    "Elements": [
                      {
                        "Label": "Product",
                        "Type": "select",
                        "Name": "Product[]",
                        "Class": "Product",
                        "SelectDisabled": true,
                        "Source": "get/Invoices/SubProducts/",
                        "SourceRequestBody": [
                          [
                            "Product",
                            [
                              "Category"
                            ]
                          ]
                        ],
                        "copyInitialValues": [
                          "multipleForm-ignore",
                          0,
                          "Parameters-ignore",
                          0,
                          "Product[]"
                        ],
                        "OnChange": {
                          "Name": "SubProduct",
                          "Source": "get/Invoices/ProductParameters/",
                          "SourceRequestBody": [
                            [
                              "Product",
                              "Product[]"
                            ]
                          ],
                          "Response": "Add",
                          "Type": "measurements"
                        },
                        "RelativeDiv": "ProductParameters",
                        "Required": 1
                      }
                    ]
                  },
                  {
                    "Type": "measurements",
                    "Label": "Measurements"
                  },
                  {
                    "Label": "Rate",
                    "Type": "text",
                    "Name": "Rate[]",
                    "Class": "money multiply",
                    "Default": "0",
                    "OnChange": {
                      "Calculate": {
                        "data": [
                          "Rate[]",
                          "Qty"
                        ],
                        "Operators": [
                          "add",
                          "multiply"
                        ],
                        "resultFill": "Net[]"
                      }
                    },
                    "Key": "Rate"
                  },
                  {
                    "Label": "Release Rate",
                    "Type": "text",
                    "renderConditionally": true,
                    "MediaOnly": true,
                    "Name": "ReleaseRate[]",
                    "Class": "money multiply",
                    "Default": "0",
                    "OnChange": {
                      "Calculate": {
                        "data": [
                          "ReleaseRate[]"
                        ],
                        "Operators": [
                          "add"
                        ],
                        "resultFill": "NetReleaseRate[]"
                      }
                    },
                    "Key": "ReleaseRate"
                  },
                  {
                    "Label": "NetReleaseRate",
                    "Type": "hidden",
                    "Name": "NetReleaseRate[]",
                    "Class": "net money",
                    "IsNumber": true,
                    "Default": "0",
                    "OnChange": {
                      "CalculateRows": {
                        "data": "NetReleaseRate[]",
                        "Operator": "add",
                        "resultFill": "TotalReleaseRate"
                      }
                    },
                    "Disabled": true,
                    "Key": "Net"
                  },
                  {
                    "Type": "parameters",
                    "Width": "200",
                    "renderConditionally": true,
                    "NonMediaOnly": true,
                    "Label": "Supplier Details",
                    "Key": "suppliers",
                    "Name": "Suppliers-ignore",
                    "Elements": [
                      {
                        "Label": "Supplier",
                        "Type": "select",
                        "IsMulti": true,
                        "SelectLimit": 1000,
                        "Name": "Supplier[]",
                        "Class": "Supplier",
                        "Source": "list/Masters/Suppliers/custom/",
                        "Required": 0
                      },
                      {
                        "Label": "Costing",
                        "Key": "Costing",
                        "Type": "percentage",
                        "Percentage": true,
                        "Class": "money",
                        "Name": "Costing[]",
                        "BaseValue": "Rate[]",
                        "Split": [
                          12,
                          12,
                          12,
                          12
                        ],
                        "OnChange": {
                          "Calculate": {
                            "data": [
                              "this"
                            ],
                            "Operators": [
                              "add"
                            ],
                            "resultFill": "NetReleaseRate[]"
                          }
                        }
                      }
                    ]
                  },
                  {
                    "Label": "Net Rate",
                    "Type": "text",
                    "Name": "Net[]",
                    "Class": "net money",
                    "OnChange": {
                      "CalculateRows": {
                        "data": "Net[]",
                        "Operator": "add",
                        "resultFill": "Total"
                      }
                    },
                    "Disabled": true,
                    "Key": "Net"
                  }
                ]
              }
            ]
          }
        },
        {
          "Size": 6,
          "Key": "formElementsFive",
          "Content_Type": "form-elements",
          "Content": {
            "Elements": [
              {
                "Type": "editor",
                "Key": "Remarks",
                "Label": "Remarks",
                "Name": "Remarks",
                "Rows": 5,
                "Placeholder": "Any important message to add"
              }
            ]
          }
        },
        {
          "Size": 6,
          "Key": "subTotalOne",
          "Content_Type": "form-elements",
          "Content": {
            "Elements": [
              {
                "Label": "Total",
                "Type": "text",
                "Percentage": false,
                "Class": "add money",
                "Name": "Total",
                "Disabled": true,
                "Key": "Total",
                "Default": "0"
              },
              {
                "Label": "Total Release Rate",
                "Type": "hidden",
                "Percentage": false,
                "Class": "add money",
                "Name": "TotalReleaseRate",
                "Disabled": true,
                "Key": "TotalReleaseRate",
                "Default": "0",
                "BaseValue": "Total",
                "OnChange": {
                  "Calculate": [
                    {
                      "BaseValues": [
                        "Total",
                        "Discount",
                        "this"
                      ],
                      "Operators": [
                        "add",
                        "subtract",
                        "subtract"
                      ],
                      "ResultFill": "EstimatedRevenue"
                    }
                  ]
                }
              },
              {
                "Label": "Discount",
                "Key": "Discount",
                "Type": "percentage",
                "Percentage": true,
                "Class": "subtract money",
                "Name": "Discount",
                "BaseValue": "Total",
                "OnChange": {
                  "Calculate": [
                    {
                      "BaseValues": [
                        "Total",
                        "TotalReleaseRate",
                        "this"
                      ],
                      "Operators": [
                        "add",
                        "subtract",
                        "subtract"
                      ],
                      "ResultFill": "EstimatedRevenue"
                    }
                  ]
                }
              },
              {
                "Label": "Estimated Revenue",
                "Type": "text",
                "Percentage": false,
                "Class": "add money",
                "Name": "EstimatedRevenue",
                "Disabled": true,
                "Key": "Revenue",
                "Default": "0.00",
                "BaseValues": [
                  "TotalReleaseRate",
                  "Discount"
                ],
                "Operators": [
                  "add",
                  "subtract"
                ]
              },
              {
                "Type": "Tax",
                "Key": "Tax",
                "Name": "Tax-Ignore",
                "Source": "get/Invoices/Tax/",
                "SourceRequestBody": [
                  [
                    "Client",
                    "Id",
                    "formData"
                  ],
                  [
                    "Category",
                    "Category"
                  ]
                ],
                "BaseValues": [
                  "Total",
                  "Discount"
                ],
                "Operators": [
                  "add",
                  "subtract"
                ],
                "OnChange": {
                  "Calculate": [
                    {
                      "BaseValues": [
                        "Total",
                        "Discount",
                        "this"
                      ],
                      "Operators": [
                        "add",
                        "subtract",
                        "add"
                      ],
                      "RoundAndFill": "RoundOff"
                    },
                    {
                      "BaseValues": [
                        "Total",
                        "Discount",
                        "this",
                        "RoundOff"
                      ],
                      "Operators": [
                        "add",
                        "subtract",
                        "add",
                        "add"
                      ],
                      "Fill": "GrandTotal"
                    }
                  ]
                }
              },
              {
                "Type": "roundOff",
                "Key": "RoundOff",
                "Label": "Round Off",
                "Name": "RoundOff",
                "Class": "add money",
                "Percentage": false,
                "Default": "0",
                "BaseValues": [
                  "Total",
                  "Discount",
                  "Tax"
                ],
                "Operators": [
                  "add",
                  "subtract",
                  "add"
                ],
                "Disabled": true
              },
              {
                "Label": "Grand Total",
                "Key": "Grand Total",
                "Type": "total",
                "Default": "0",
                "Percentage": false,
                "Name": "GrandTotal",
                "BaseValues": [
                  "Total",
                  "Discount",
                  "Tax",
                  "RoundOff"
                ],
                "Operators": [
                  "add",
                  "subtract",
                  "add",
                  "add"
                ],
                "Disabled": true
              }
            ]
          }
        }
      ]
    }
  }
